{
  "expo": {
    "name": "",
    "slug": "PluckMate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/color-logo.png",
    "scheme": "pluckmatemobile",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1F4D33"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/logo.png",
        "backgroundColor": "#1F4D33"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo/color-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo/logo.png",
          "imageWidth": 1000,
          "backgroundColor": "#1F4D33"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}